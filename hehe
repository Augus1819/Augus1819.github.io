	var handshakeLock1Lock=4070984158224;
	var loginDivName = "#notsetyet";

	var fb_popup;	
	function receiveMessage(event){
		console.log("receiveMessage ",event);
		if ( 	fb_popup && 
				event.origin=="https://agarz.com" &&
				event.data ){
			$(loginDivName).html(event.data.txt);
			if ( event.data.ban==0 ){
				localStorage.userToken = event.data.token;
				ajax_skinFavori();
				ajax_isimFavori();
			}else{
				localStorage.userToken = null;
			}
			$('#nick').val(event.data.name_default);
			$('#txtSkin').val(event.data.skin_default);
			fb_popup.close();
		}
	}
	window.addEventListener("message", receiveMessage, false);	
	var fb_popup_show = function(){
		$(loginDivName).html("");
		fb_popup = window.open('//agarz.com/fb_popup','fb login popup','height=800,width=800');
        if (window.focus) {
			fb_popup.focus()
		}
        return false;
	}	
	var user_rememberMe = function (cb,token){
		if ( cb.checked==true ){
			localStorage.userToken=token;
		}else{
			localStorage.userToken=null;
		}
	}
	var ajax_isimFavori = function (){
		console.log("ajax_isimFavori");
		$.ajax( {url:'/ajax_isimFavori',
				method: "POST",
				data: {token:localStorage.userToken},
				cache:false} )
				.done(function(msg) {
					if ( msg=='' ) return;
					
					var isimList = JSON.parse(msg);
					
					$(".isim-popup").html("");
					
					$(".isim-popup").append("<a href='' class='isim-link isim-kapat'>X</a>");
					for(var i=0; i<isimList.length; i++){
						var isim = isimList[i];
						if ( (i%5)==0 && i>0 ){
							$(".isim-popup").append("<br>");
						}
						$(".isim-popup").append("<a href='' class='isim-a isim-link'>"+isim+"</a>");
					}
					
					if ( isimList.length>0 ){
						$('#isimFavori').click( function(){
							$(".isim-popup").addClass("active");
						});
						$('.isim-a').click( function(e){
							$("#nick").val(e.target.innerText);
							$(".isim-popup").removeClass("active");
							return false;
						});
						$('.isim-kapat').click( function(e){
							$(".isim-popup").removeClass("active");
							return false;
						});
					}
				});
	}
	var ajax_skinFavori = function (){
		console.log("ajax_skinFavori");
		$.ajax( {url:'/ajax_skinFavori',
				method: "POST",
				data: {token:localStorage.userToken},
				cache:false} )
				.done(function(msg) {
					var skinList = JSON.parse(msg);
					
					$(".skin-popup").html("");
											
					$(".skin-popup").append("<a href='' class='isim-link skin-kapat'>X</a>");
					for(var i=0; i<skinList.length; i++){
						var skinName = skinList[i];
						$(".skin-popup").append("<img src='//cdn.agarz.com/"+skinName.skin+"' class='skin-popup-close skin-img' alt='"+skinName.isim+"' width='128' height='128'>");
					}
					
					if ( skinList.length>0 ){
						$('#skinFavori').click( function(){
							$(".skin-popup").addClass("active");
						});
						$('.skin-popup-close').click( function(){
							$(".skin-popup").removeClass("active");
						});
						$('.skin-img').click( function(e){
							$("#txtSkin").val(e.target.alt);
						});
						$('.skin-kapat').click( function(e){
							$(".skin-popup").removeClass("active");
							return false;
						});
					}
				});
	}
	var user_token = function (){
		console.log("user_token "+loginDivName);
		$.ajax( {url:ajax_url,
				method: "POST",
				data: {	me:'token',
						token:localStorage.userToken, 
						isMobile:isMobile },
				cache:false} )
			  .done(function(msg) {
				  $(loginDivName).html(msg);
				  
				  var nd = $("#idNameDefault").html();
				  $("#nick").val(nd);
				  
				  var sd = $("#idSkinDefault").html();
				  $("#txtSkin").val(sd);
				  
				  ajax_skinFavori();
				  ajax_isimFavori();
			  });
	}
	var user_show = function (){
		console.log("user_show "+loginDivName);
		$.ajax( {url:ajax_url,
				method: "POST",
				data: {	me:'show',
						isMobile:isMobile },
				cache:false} )
			  .done(function(msg) {
				  $(loginDivName).html(msg);
				  $('loginButton').click( function(){alert('j')} );
			  });
	}
	var user_login = function (){
		console.log("user_login "+loginDivName);
		var email = $("#email").val();
		var pass = $("#password").val();
		$.ajax( {url:ajax_url,
				method: "POST",
				data: {	me:'login', 
						email:email, 
						password:pass,
						isMobile:isMobile },
				cache:false} )
			  .done(function(msg) {
				  $(loginDivName).html(msg);
				  
				  var nd = $("#idNameDefault").html();
				  $("#nick").val(nd);
				  
				  var sd = $("#idSkinDefault").html();
				  $("#txtSkin").val(sd);
				  
				  localStorage.userToken = $("#userToken").html();
				  
				  ajax_skinFavori();
				  ajax_isimFavori();
			  });
	}
	var user_logout = function (token){
		console.log("user_logout "+token);
		localStorage.userToken=null;
		$.ajax( {url:ajax_url,
				method: "POST",
				data: {	me:'logout',
						token:token,
						isMobile:isMobile },
				cache:false} )
			  .done(function(msg) {
				  user_show();
				  $('#nick').val('');
				  $('#txtSkin').val('');
				  
				  $(".skin-popup").html("");
				  $(".skin-popup").removeClass("active");
				  
				  $(".isim-popup").html("");
				  $(".isim-popup").removeClass("active");
				  
				  ajax_skinFavori();
			  });
	}
	
	
	</script>
